package tset001;

import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
			System.out.println("문자열 입력 :");
			String str = sc.nextLine();
			if(str == "e") {
				System.out.println("프로그램 종료");
			}else {
				System.out.println(str.length() + "글자입니다.");
			}
		
		
	}

}
