package tset001;

public class Fruit {
    private String name;
    private String color;

    public Fruit(String name) {
        this.name = name;
        this.color = color;
    }
    
    

    public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	public String getColor() {
		return color;
	}



	public void setColor(String color) {
		this.color = color;
	}



	@Override
    public String toString() {
        return name + " (" + color + ")";
    }
}


}
